{
    "log_file": "/var/log/dahbug.log",
    "output": "file",
    "label_format": "[%s]",
    "string_format": "'%s'",
    "timestamp_format": "Y-m-d H:i:s",
    "print_timestamp": true,
    "print_filename": true,
    "string_cap": 50,
    "line_endings": "LF",
    "output_encoding": "UTF-8",
    "indent": 4,
    "max_depth": 3,
    "use_colors": true,
    "theme": "default",
    "color": {
        "red": "0;31",
        "light_red": "1;31",
        "green": "0;32",
        "light_green": "1;32",
        "cyan": "0;36",
        "light_cyan": "1;36",
        "blue": "0;34",
        "light_blue": "1;34",
        "purple": "0;35",
        "light_purple": "1;35",
        "yellow": "1;33",
        "white": "1;37"
    }
}
