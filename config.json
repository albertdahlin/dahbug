{
    "log_file": "/var/log/dahbug.log",
    "output": "file",
    "label_format": "[%s]",
    "string_format": "'%s'",
    "timestamp_format": "Y-m-d H:i:s",
    "print_timestamp": true,
    "print_filename": true,
    "string_cap": 50,
    "ascii_notation": "escape",
    "line_endings": "LF",
    "output_encoding": "UTF-8",
    "indent": 4,
    "max_depth": 3,
    "use_colors": true,
    "theme": "default",
    "color": {
        "black": "0;0",
        "grey": "1;0",
        "red": "0;1",
        "light_red": "1;1",
        "green": "0;2",
        "light_green": "1;2",
        "yellow": "1;3",
        "brown": "0;3",
        "blue": "0;4",
        "light_blue": "1;4",
        "purple": "0;5",
        "light_purple": "1;5",
        "cyan": "0;6",
        "light_cyan": "1;6",
        "white": "1;7",
        "light_grey": "0;7"
    },
    "escape_chars": {
        "0": "\\0",
        "7": "\\a",
        "8": "\\b",
        "9": "\\t",
        "10": "\\n",
        "11": "\\v",
        "12": "\\f",
        "13": "\\r",
        "27": "\\e"
    }
}
